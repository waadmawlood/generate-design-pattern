{
    "name": "waad/generate-design-pattern",
    "description": "package to generate repository design pattern dev",
    "keywords": [
        "Waad Mawlood",
        "repository",
        "laravel",
        "design pattetn",
        "laravel-php",
        "repo"
    ],
    "homepage": "https://github.com/waadmawlood/generate-design-pattern",
    "license": "MIT",
    "authors": [
        {
            "name": "Waad Mawlood",
            "email": "waad_mawlood@outlook.com",
            "homepage": "https://waad.netlify.app",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.12.0"
    },
    "autoload": {
        "psr-4": {
            "Waad\\Repository\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Waad\\Repository\\RepositoryServiceProvider"
            ]
        }
    }
}
